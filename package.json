    "gulp-exec": "3.0.2",
    "gulp-git": "2.8.1",
    "gulp-git-streamed": "2.8.1",
    "gulp-if": "2.0.2",
    "gulp-prompt": "1.2.0",
    "gulp-sass": "4.0.2",
    "gulp-sass-lint": "1.4.0",
    "gulp-util": "3.0.8",
    "husky": "^1.2.0",
    "jest": "^24.1.0",
    "js-cookie": "^2.1.1",
    "npm-which": "^3.0.1",
    "object-to-spawn-args": "^1.1.1",
    "script-loader": "^0.7.0",
    "sinon": "^7.1.1",
    "webpack": "^4.29.1",
    "webpack-livereload-plugin": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
