{
  "name": "@bigcommerce/stencil-cli",
  "version": "1.15.5",
  "description": "CLI tool to run BigCommerce Stores locally for theme development.",
  "main": "index.js",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run run-tests",
    "run-tests": "lab bin/ server/ lib/ tasks/ -t 80 -l -e 'development' -P spec",
    "test-cov-html": "lab bin/ server/ lib/ -r html -o coverage.html -P spec",
    "dev": "webpack",
    "deploy": "webpack --deploy",
    "install-private-dependencies": "npm install git+ssh://git@github.com:bigcommerce-labs/jschannel.git#master git+ssh://git@github.com:bigcommerce-labs/ng-common.git#master git+ssh://git@github.com:bigcommerce-labs/ng-stencil-editor.git#master git+ssh://git@github.com:bigcommerce-labs/stencil-preview-sdk.git#master git+ssh://git@github.com:bigcommerce-labs/bcapp-pattern-lab.git#master",
    "uninstall-private-dependencies": "npm uninstall jschannel ng-common ng-stencil-editor stencil-preview-sdk bcapp-pattern-lab"
  },
  "bin": {
    "stencil": "./bin/stencil",
    "stencil-init": "./bin/stencil-init",
    "stencil-push": "./bin/stencil-push",
    "stencil-start": "./bin/stencil-start",
    "stencil-release": "./bin/stencil-release"
  },
  "config": {
    "stencil_version": "1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bigcommerce/stencil-cli"
  },
  "author": "BigCommerce",
  "license": "BSD-4-Clause",
  "bugs": {
    "url": "https://github.com/bigcommerce/stencil-cli/issues"
  },
  "homepage": "https://github.com/bigcommerce/stencil-cli",
  "dependencies": {
    "@bigcommerce/stencil-paper": "^3.0.0-rc.10",
    "@bigcommerce/stencil-styles": "1.1.0",
    "accept-language-parser": "^1.0.2",
    "archiver": "^3.0.0",
    "async": "^2.4.0",
    "babel-eslint": "^10.0.1",
    "boom": "^7.3.0",
    "browser-sync": "git://github.com/bigcommerce/browser-sync.git",
    "cheerio": "^1.0.0-rc.2",
    "code": "^5.2.4",
    "colors": "^1.0.3",
    "commander": "^2.7.1",
    "confidence": "^4.0.1",
    "dateformat": "^3.0.3",
    "decompress-zip": "^0.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "front-matter": "^3.0.1",
    "github": "^14.0.0",
    "glob": "^7.1.3",
    "glue": "^6.0.0",
    "good": "^8.1.2",
    "good-console": "^7.1.0",
    "graceful-fs": "^4.1.11",
    "handlebars": "^4.0.12",
    "hapi": "^18.1.0",
    "hoek": "^6.1.2",
    "image-size": "^0.7.1",
    "inquirer": "^6.2.2",
    "install": "^0.12.2",
    "jsonlint": "^1.6.2",
    "jsonschema": "^1.0.2",
    "jspm": "^0.16.53",
    "jspm-git": "^0.6.0",
    "lab": "^18.0.1",
    "less": "^3.9.0",
    "lodash": "^4.17.11",
    "memory-cache": "^0.2.0",
    "npm": "^6.7.0",
