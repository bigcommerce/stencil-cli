{
  "parserOptions": {
    "ecmaVersion": 8
  },
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "Set": true
  },
  "rules": {
    "curly": 2,
    "no-eq-null": 2,
    "comma-dangle": [ 2, "always-multiline" ],
    "no-caller": 2,
    "dot-notation": 0,
    "no-debugger": 2,
    "no-undef": 2,
    "no-unused-vars": 2
  }
}
